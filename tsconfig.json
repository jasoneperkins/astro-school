{
	"compilerOptions": {
		"paths": {
			"@assets/*": ["./src/assets/*"],
			"@components/*": ["./src/components/*"],
			"@icons/*": ["./src/assets/icons/*"],
			"@images/*": ["./src/assets/images/*"],
			"@landmarks/*": ["./src/landmarks/*"],
			"@layouts/*": ["./src/layouts/*"],
			"@masks/*": ["./src/assets/masks/*"],
			"@public/*": ["./public/*"],
			"@styles/*": ["./src/styles/*"]
		}
	},
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"]
}
